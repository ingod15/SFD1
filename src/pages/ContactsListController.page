<apex:page controller="ContactsListController">
    <apex:form>
        <apex:pageBlock title="Contacts List" id="contacts_list">
            <apex:pageBlockTable value="{! contacts }" var="ct">
                <apex:column value="{! ct.FirstName }"/>



            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:form>
</apex:page>
